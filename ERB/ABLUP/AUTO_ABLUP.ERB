@AUTO_ABLUP
#LOCALSIZE 0
#LOCALSSIZE 0
#DIM ABL_NUM
#DIM FIRST_LV
PRINTL 자동으로 레벨업합니다
REPEAT 24
    ABL_NUM = GET_ABL_NUMBER(COUNT)
    IF ABL_NUM != -1
        FIRST_LV = ABL:ABL_NUM
        CALLFORM ISABLUP{ABL_NUM}
        WHILE RESULT
            CALLFORM AUTO_ABLUP{ABL_NUM}
            SIF !RESULT
                BREAK
            CALLFORM ISABLUP{ABL_NUM}
        WEND
        SIF ABL:ABL_NUM > FIRST_LV
            PRINTFORML [%ABLNAME:ABL_NUM%] {FIRST_LV} -> {ABL:ABL_NUM}
    ENDIF
REND
PRINTW 레벨업 완료

@SHOW_AUTO_ABLUP_BUTTON
#LOCALSIZE 0
#LOCALSSIZE 0
PRINTL [101] 자동 레벨업

@IS_AUTO_ABLUP_VALUE(ARG)
#FUNCTION
#LOCALSIZE 0
#LOCALSSIZE 0
RETURNF ARG == 101