@LIBRARY_BOOK_HAVE(ARG)
#FUNCTION
RETURNF LIBRARY_BOOKS:ARG > 0

@LIBRARY_BOOK_EXIST(ARG)
#FUNCTION
TRYCALLFORM BOOK_{ARG}
RETURNF RESULT

@LIBRARY_BOOK_TITLE(ARG)
#FUNCTIONS
CALLFORM BOOK_{ARG}
RETURNF RESULTS

@LIBRARY_BOOK_READ(ARG)
CALLFORM BOOK_{ARG}_READ

@LIBRARY_BOOK_EFFECT_ALL(ARGS)
REPEAT LIBRARY_BOOK_MAX
    IF LIBRARY_BOOK_HAVE(COUNT)
        CALL LIBRARY_BOOK_EFFECT(ARGS, COUNT)
    ENDIF
REND

@LIBRARY_BOOK_EFFECT(ARGS, ARG)
TRYCALLFORM BOOK_{ARG}_EFFECT_%ARGS%
