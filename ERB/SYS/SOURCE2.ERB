LOCALS:9 = 

;絶頂寸止め
REPEAT 4
    LOCAL:99 = UP:COUNT + PALAM:COUNT
    IF LOCAL:99 >= 1000000
        NOWEX:(20 + COUNT) = 9
    ELSEIF LOCAL:99 >= 200000
        NOWEX:(20 + COUNT) = 4
    ELSEIF LOCAL:99 >= PALAMLV:4 * 2
        NOWEX:(20 + COUNT) = 2
    ELSEIF LOCAL:99 >= PALAMLV:4
        NOWEX:(20 + COUNT) = 1
    ENDIF
REND

IF (NOWEX:20 || NOWEX:21 || NOWEX:22 || NOWEX:23) && NOWEX:41 == 0
    ;焦らし中自慰の判定を各部位ごとに行う
    TFLAG:180 = 0
    TFLAG:181 = 0
    TFLAG:182 = 0
    TFLAG:183 = 0
    LOCAL = -20
    ;自慰が我慢できない素質
    ;プライド低い
    SIF TALENT:17
        LOCAL += 5
    ;自慰しやすい
    SIF TALENT:60
        LOCAL += 10
    ;快感に素直
    SIF TALENT:70
        LOCAL += 2
    ;淫核／淫茎
    SIF TALENT:171
        TFLAG:180 += 5
    ;淫壷
    SIF TALENT:172
        TFLAG:181 += 5
    ;淫尻
    SIF TALENT:173
        TFLAG:182 += 5
    ;淫乳
    SIF TALENT:174
        TFLAG:183 += 5
    ;Ｃ敏感
    SIF TALENT:100
        TFLAG:180 += 3
    ;Ｖ敏感
    SIF TALENT:102
        TFLAG:181 += 3
    ;Ａ敏感
    SIF TALENT:104
        TFLAG:182 += 3
    ;Ｂ敏感
    SIF TALENT:106
        TFLAG:183 += 3
    ;幼稚
    SIF TALENT:131
        LOCAL += 3

    ;サドっ気
    LOCAL -= ABL:20

    ;むしろ我慢するのが好きだったりする素質
    ;気丈
    SIF TALENT:12
        LOCAL -= 3
    ;プライド高い
    SIF TALENT:16
        LOCAL -= 5
    ;好奇心
    SIF TALENT:23
        LOCAL -= 5
    ;快感の否定
    SIF TALENT:71
        LOCAL += 5
    ;倒錯的
    SIF TALENT:80
        LOCAL -= 5
    ;中毒しやすい
    SIF TALENT:84
        LOCAL += 5

    ;マゾっ気
    LOCAL -= ABL:21 * 2
    ;従順
    LOCAL -= ABL:10 * 2
    ;自慰中毒
    LOCAL += ABL:30 * 10

    ;欲情
    IF PALAM:13 >= PALAMLV:9
        LOCAL += 20
    ELSEIF PALAM:13 >= PALAMLV:8
        LOCAL += 16
    ELSEIF PALAM:13 >= PALAMLV:7
        LOCAL += 14
    ELSEIF PALAM:13 >= PALAMLV:6
        LOCAL += 12
    ELSEIF PALAM:13 >= PALAMLV:5
        LOCAL += 10
    ELSEIF PALAM:13 >= PALAMLV:4
        LOCAL += 8
    ELSEIF PALAM:13 >= PALAMLV:3
        LOCAL += 5
    ELSEIF PALAM:13 >= PALAMLV:2
        LOCAL += 2
    ENDIF

    ;寸止め継続回数
    TFLAG:180 += EX:20 * 5 + LOCAL
    TFLAG:181 += EX:21 * 5 + LOCAL
    TFLAG:182 += EX:22 * 5 + LOCAL
    TFLAG:183 += EX:23 * 5 + LOCAL

    ;拘束中は無条件にクリア
    IF TEQUIP:40 || TEQUIP:71
        TFLAG:180 = 0
        TFLAG:181 = 0
        TFLAG:182 = 0
        TFLAG:183 = 0
    ENDIF

    LOCAL:0 = TFLAG:180
    LOCAL:1 = TFLAG:181
    LOCAL:2 = TFLAG:182
    LOCAL:3 = TFLAG:183

    ;絶頂寸止め判定
    REPEAT 4
        SIF NOWEX:(20+COUNT) < 1
            CONTINUE
        IF NOWEX:(20+COUNT) == 9
            TIMES TFLAG:180 , 3.00
            LOCALS:8 = 최강
        ELSEIF NOWEX:(20+COUNT) == 4
            TIMES TFLAG:180 , 2.00
            LOCALS:8 = 초강
        ELSEIF NOWEX:(20+COUNT) == 2
            TIMES TFLAG:180 , 1.50
            LOCALS:8 = 강
        ELSE
            NOWEX:(20+COUNT) = 1
            LOCALS:8 = 
        ENDIF
        LOCAL:90 = RAND:100
        IF LOCAL:COUNT > LOCAL:90
            NOWEX:42 += NOWEX:(20+COUNT)
        ELSE
            EX:(20+COUNT) += NOWEX:(20+COUNT)
            UP:COUNT = 0
            PALAM:COUNT = PALAMLV:4 - 1
            LOCALS:9 = %LOCALS:9 + LOCALS:8 + LOCALS:COUNT%　
        ENDIF
    REND
ENDIF
SIF STRLENS(LOCALS:9) > 0
